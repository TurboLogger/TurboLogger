{
  "name": "@oxog/turbologger",
  "version": "1.0.0",
  "description": "Ultra-fast, zero-config Node.js logging library with modern observability features",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib/",
    "types/",
    "tools/cli.js",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "clean": "node scripts/build.js clean",
    "prebuild": "npm run clean",
    "build": "node scripts/build.js build",
    "build:dev": "node scripts/build.js dev",
    "build:prod": "node scripts/build.js prod",
    "build:cjs": "node scripts/build.js compile",
    "build:esm": "node scripts/build.js compileEsm",
    "build:watch": "node scripts/build.js watch",
    "test": "node scripts/test-runner.js",
    "test:watch": "node scripts/test-runner.js --watch",
    "test:basic": "node scripts/test-runner.js --basic",
    "lint": "node scripts/build.js lint",
    "lint:fix": "npx eslint src/index.ts src/simple-logger.ts --fix",
    "format": "node scripts/build.js format",
    "format:check": "npx prettier --check \"src/index.ts\" \"src/simple-logger.ts\"",
    "typecheck": "node scripts/build.js typecheck",
    "prepublishOnly": "npm run build:prod",
    "prepare": "npm run build:prod || npm run build",
    "prepack": "npm run build",
    "demo:advanced": "node examples/advanced-features-demo.js",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release:patch": "npm version patch",
    "release:minor": "npm version minor",
    "release:major": "npm version major"
  },
  "bin": {
    "oxog-turbologger": "./tools/cli.js"
  },
  "keywords": [
    "logger",
    "logging",
    "log",
    "fast",
    "performance",
    "high-performance",
    "observability",
    "tracing",
    "metrics",
    "monitoring",
    "cloud-native",
    "nodejs",
    "node",
    "json",
    "structured-logging",
    "pino-alternative",
    "winston-alternative",
    "async",
    "streaming",
    "elasticsearch",
    "cloudwatch",
    "stackdriver",
    "azure-monitor"
  ],
  "author": {
    "name": "Ersin KoÃ§",
    "email": "ersinkoc@gmail.com",
    "url": "https://github.com/TurboLogger/TurboLogger"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TurboLogger/TurboLogger.git"
  },
  "bugs": {
    "url": "https://github.com/TurboLogger/TurboLogger/issues"
  },
  "homepage": "https://github.com/TurboLogger/TurboLogger#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "commander": "^11.1.0",
    "eslint": "^8.0.0",
    "glob": "^10.3.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {},
  "optionalDependencies": {
    "@elastic/elasticsearch": "^8.0.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.0.0",
    "@google-cloud/logging": "^11.0.0",
    "@azure/monitor-opentelemetry-exporter": "^1.0.0",
    "ws": "^8.14.0",
    "zod": "^3.22.0",
    "fastify": "^4.24.0"
  }
}
